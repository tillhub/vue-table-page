{
  "name": "@tillhub/vue-table-page",
  "version": "3.16.2",
  "description": "Extended pagination based on Element-UI.",
  "author": "Jessica Kellogg",
  "private": false,
  "main": "./dist/vue-table-page.common.js",
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --port 4001 ./example/main.js",
    "build:lib": "vue-cli-service build --target lib --name vue-table-page src/components/VueTablePage.vue",
    "build": "npm run clean && npm run build:lib",
    "clean": "rimraf dist",
    "lint": "vue-cli-service lint",
    "semantic-release": "semantic-release",
    "precommit": "git add .",
    "commit": "npm run precommit && git-cz"
  },
  "dependencies": {
    "currency-formatter": "^1.5.5",
    "just-safe-get": "^1.3.2",
    "v-calendar": "^0.9.7",
    "vue-i18n": "^8.14.0"
  },
  "peerDependencies": {
    "vue": "^2.6.10",
    "element-ui": "^2.11.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.4.2",
    "@semantic-release/npm": "^5.1.13",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.23",
    "cz-conventional-changelog": "^3.0.2",
    "element-ui": "^2.11.1",
    "gh-pages": "^2.1.1",
    "rimraf": "^2.7.0",
    "semantic-release": "^15.13.21",
    "validate-commit-msg": "^2.14.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/npm",
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/git",
        "assets": [
          "dist",
          "package.json",
          "package-lock.json",
          "src",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
